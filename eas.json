{
  "cli": {
    "version": ">= 3.22.0"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}",
        "BACKEND_URL": "${BACKEND_URL}"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "distribution": "store",
      "channel": "production",
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}",
        "BACKEND_URL": "${BACKEND_URL}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
