{
  "name": "trafficlites-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "npx eas-cli build --platform android --profile preview --non-interactive",
    "build:ios": "npx eas-cli build --platform ios --profile preview --local",
    "build:web": "npx expo export -p web",
    "lint": "echo \"Linting placeholder - configure your linter here\"",
    "test": "echo \"Testing placeholder - configure your test runner here\"",
    "build:all": "npm run lint && npm run test && npm run build:web && npm run build:android && npm run build:ios",
    "submit:android": "npx eas-cli submit -p android --latest",
    "submit:ios": "npx eas-cli submit -p ios --latest",
    "submit:all": "npm run submit:android && npm run submit:ios"
  },
  "dependencies": {
    "expo": "~51.0.2",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-location": "~17.0.1",
    "expo-sqlite": "~14.0.3",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.28",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-web": "~0.19.10",
    "react-dom": "18.2.0",
    "@expo/metro-runtime": "~3.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@expo/config-plugins": "8.0.0",
    "@expo/prebuild-config": "7.0.0"
  },
  "private": true
}
